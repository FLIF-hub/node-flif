{
  "name": "node-flif",
  "version": "0.2.0",
  "flifVersion": "0.3.0",
  "description": "A Node.js wrapper for the FLIF executable.",
  "main": "index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "debug": "node --inspect-brk test.js",
    "test-exe": "node test.js --exe",
    "test-wasm": "node test.js --wasm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FLIF-hub/node-flif.git"
  },
  "keywords": [
    "flif",
    "node",
    "lossless",
    "image-compression",
    "image-conversion",
    "encode",
    "decode"
  ],
  "dependencies": {
    "flif-wasm": "^1.0.6",
    "root-require": "^0.3.1"
  },
  "author": "The Jared Wilcurt",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/FLIF-hub/node-flif/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/FLIF-hub/node-flif/issues"
  },
  "homepage": "https://github.com/FLIF-hub/node-flif#readme"
