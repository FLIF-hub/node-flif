{
  "name": "node-flif",
  "version": "0.1.0",
  "flif-version": "0.3.0",
  "description": "A Node.js wrapper for the FLIF executable.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "installer": "npm install --loglevel=error",
    "lint": "eslint --config=.eslintrc.js index.js",
    "fix": "eslint --fix --config=.eslintrc.js _scripts gulpfile.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FLIF-hub/node-flif.git"
  },
  "keywords": [
    "flif",
    "node",
    "lossless",
    "image-compression",
    "image-conversion",
    "encode",
    "decode"
  ],
  "devDependencies": {
    "eslint": "^3.8.1",
    "pre-commit": "^1.1.3"
  },
  "author": "The Jared Wilcurt",
  "license": "MIT",
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/FLIF-hub/node-flif/blob/master/LICENSE"
  }],
  "bugs": {
    "url": "https://github.com/FLIF-hub/node-flif/issues"
  },
  "homepage": "https://github.com/FLIF-hub/node-flif#readme",
  "pre-commit": [ "lint" ]
}
